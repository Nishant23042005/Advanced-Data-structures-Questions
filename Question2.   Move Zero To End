public class Main {
    public static void main(String[] args) {
        Solution sol = new Solution();
        
        int[] nums = {0, 1, 0, 3, 12};  
        sol.moveZeroes(nums);

       
        System.out.print("Output: ");
        for (int num : nums) {
            System.out.print(num + " ");
        }
    }
}

class Solution {
    public void moveZeroes(int[] nums) {
        int k = 0;
        for (int i = 0; i < nums.length; i++) {
            if (nums[i] != 0) {
                nums[k] = nums[i];
                k++;
            }
        }
        while (k < nums.length) {
            nums[k] = 0;
            k++;
        }
    }
}
